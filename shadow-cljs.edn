;; shadow-cljs configuration
{:source-paths
 ["src/dev"
  "src/main"
  "src/test"
  "src/stories"]

 :dependencies
 [[reagent "1.1.0"]
  [re-frame "1.2.0"]]

 :dev-http
 {8080 "public"}

 :builds
 {:app {:target     :browser
        :output-dir "public/js"
        :modules    {:main {:init-fn app.core/init}}
        :js-options {:js-provider    :external
                     :external-index "target/index.js"}}}}


